<h1>
  <%= TradeParty.model_name.human(count: 2) %>
</h1>

<div class="table-responsive">
  <table class="table table-hover table-striped table-bordered" id="trade_parties">
    <thead>
      <tr>
        <th class="no-wrap">
          <%= TradeParty.human_attribute_name(:name) %>
        </th>
        <th>
          <%= TradeParty.human_attribute_name(:city) %>
        </th>
        <th>
          <%= TradeParty.human_attribute_name(:email) %>
        </th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <%= render partial: "trade_party", collection: @trade_parties %>
    </tbody>
  </table>

</div>

<%= link_to "New trade partner", new_trade_party_path %>
